local B={"\065\108\108"}local D=game:GetService("\080\108\097\121\101\114\115")local V=D.LocalPlayer local Q=false local k=function(B)B=B:lower()if B=="\097\108\108"or B=="\111\116\104\101\114\115"then Q=true return elseif B=="\114\097\110\100\111\109"then local B=D:GetPlayers()if table.find(B,V)then table.remove(B,table.find(B,V))end return B[math.random(#B)]elseif B~="\114\097\110\100\111\109"and(B~="\097\108\108"and B~="\111\116\104\101\114\115")then for D,Q in next,D:GetPlayers()do if Q~=V then if(Q.Name:lower()):match("\094"..B)then return Q elseif(Q.DisplayName:lower()):match("\094"..B)then return Q end end end else return end end local J=function(B,D,V)(game:GetService("\083\116\097\114\116\101\114\071\117\105")):SetCore("\083\101\110\100\078\111\116\105\102\105\099\097\116\105\111\110",{Title=B,Text=D;Duration=V})end local W=function(B)local k=V.Character local W=k and k:FindFirstChildOfClass("\072\117\109\097\110\111\105\100")local P=W and W.RootPart local C=B.Character local Y local A local f local X local e if C:FindFirstChildOfClass("\072\117\109\097\110\111\105\100")then Y=C:FindFirstChildOfClass("\072\117\109\097\110\111\105\100")end if Y and Y.RootPart then A=Y.RootPart end if C:FindFirstChild("\072\101\097\100")then f=C.Head end if C:FindFirstChildOfClass("\065\099\099\101\115\115\111\114\121")then X=C:FindFirstChildOfClass("\065\099\099\101\115\115\111\114\121")end if X and X:FindFirstChild("\072\097\110\100\108\101")then e=X.Handle end if k and(W and P)then if P.Velocity.Magnitude<50 then(getgenv()).OldPos=P.CFrame end if Y and(Y.Sit and not Q)then return J("\069\114\114\111\114 \079\099\099\117\114\114\101\100","\084\097\114\103\101\116\105\110\103 \105\115 \115\105\116\116\105\110\103",5)end if f then workspace.CurrentCamera.CameraSubject=f elseif not f and e then workspace.CurrentCamera.CameraSubject=e elseif Y and A then workspace.CurrentCamera.CameraSubject=Y end if not C:FindFirstChildWhichIsA("\066\097\115\101\080\097\114\116")then return end local V=function(B,D,V)P.CFrame=(CFrame.new(B.Position)*D)*V k:SetPrimaryPartCFrame((CFrame.new(B.Position)*D)*V)P.Velocity=Vector3.new(9000000000,90000000000,9000000000)P.RotVelocity=Vector3.new(9000000000,9000000000,9000000000)end local n=function(Q)local k=2 local J=tick()local f=0 repeat if P and Y then if Q.Velocity.Magnitude<50 then f=f+100 V(Q,CFrame.new(0,1.5,0)+(Y.MoveDirection*Q.Velocity.Magnitude)/1.25,CFrame.Angles(math.rad(f),0,0))task.wait()V(Q,CFrame.new(0,-1.5,0)+(Y.MoveDirection*Q.Velocity.Magnitude)/1.25,CFrame.Angles(math.rad(f),0,0))task.wait()V(Q,CFrame.new(2.25,1.5,-2.25)+(Y.MoveDirection*Q.Velocity.Magnitude)/1.25,CFrame.Angles(math.rad(f),0,0))task.wait()V(Q,CFrame.new(-2.25,-1.5,2.25)+(Y.MoveDirection*Q.Velocity.Magnitude)/1.25,CFrame.Angles(math.rad(f),0,0))task.wait()V(Q,CFrame.new(0,1.5,0)+Y.MoveDirection,CFrame.Angles(math.rad(f),0,0))task.wait()V(Q,CFrame.new(0,-1.5,0)+Y.MoveDirection,CFrame.Angles(math.rad(f),0,0))task.wait()else V(Q,CFrame.new(0,1.5,Y.WalkSpeed),CFrame.Angles(math.rad(90),0,0))task.wait()V(Q,CFrame.new(0,-1.5,-Y.WalkSpeed),CFrame.Angles(0,0,0))task.wait()V(Q,CFrame.new(0,1.5,Y.WalkSpeed),CFrame.Angles(math.rad(90),0,0))task.wait()V(Q,CFrame.new(0,1.5,A.Velocity.Magnitude/1.25),CFrame.Angles(math.rad(90),0,0))task.wait()V(Q,CFrame.new(0,-1.5,-A.Velocity.Magnitude/1.25),CFrame.Angles(0,0,0))task.wait()V(Q,CFrame.new(0,1.5,A.Velocity.Magnitude/1.25),CFrame.Angles(math.rad(90),0,0))task.wait()V(Q,CFrame.new(0,-1.5,0),CFrame.Angles(math.rad(90),0,0))task.wait()V(Q,CFrame.new(0,-1.5,0),CFrame.Angles(0,0,0))task.wait()V(Q,CFrame.new(0,-1.5,0),CFrame.Angles(math.rad(-90),0,0))task.wait()V(Q,CFrame.new(0,-1.5,0),CFrame.Angles(0,0,0))task.wait()end else break end until Q.Velocity.Magnitude>500 or Q.Parent~=B.Character or B.Parent~=D or not B.Character==C or Y.Sit or W.Health<=0 or tick()>J+k end workspace.FallenPartsDestroyHeight=0/0 local G=Instance.new("\066\111\100\121\086\101\108\111\099\105\116\121")G.Name="\069\112\105\120\086\101\108"G.Parent=P G.Velocity=Vector3.new(9000000000,9000000000,9000000000)G.MaxForce=Vector3.new(1/0,1/0,1/0)W:SetStateEnabled(Enum.HumanoidStateType.Seated,false)if A and f then if(A.CFrame.p-f.CFrame.p).Magnitude>5 then n(f)else n(A)end elseif A and not f then n(A)elseif not A and f then n(f)elseif not A and(not f and(X and e))then n(e)else return J("\069\114\114\111\114 \079\099\099\117\114\114\101\100","\084\097\114\103\101\116 \105\115 \109\105\115\115\105\110\103 \101\118\101\114\121\116\104\105\110\103",5)end G:Destroy()W:SetStateEnabled(Enum.HumanoidStateType.Seated,true)workspace.CurrentCamera.CameraSubject=W repeat P.CFrame=(getgenv()).OldPos*CFrame.new(0,.5,0)k:SetPrimaryPartCFrame((getgenv()).OldPos*CFrame.new(0,.5,0))W:ChangeState("\071\101\116\116\105\110\103\085\112")table.foreach(k:GetChildren(),function(B,D)if D:IsA("\066\097\115\101\080\097\114\116")then D.Velocity,D.RotVelocity=Vector3.new(),Vector3.new()end end)task.wait()until(P.Position-(getgenv()).OldPos.p).Magnitude<25 workspace.FallenPartsDestroyHeight=(getgenv()).FPDH else return J("\069\114\114\111\114 \079\099\099\117\114\114\101\100","\082\097\110\100\111\109 \101\114\114\111\114",5)end end if not Welcome then J("\083\099\114\105\112\116 \098\121 \065\110\116\104\111\110\121\073\115\110\116\072\101\114\101","\069\110\106\111\121\033",5)end;(getgenv()).Welcome=true if B[1]then for B,D in next,B do k(D)end else return end if Q then for B,D in next,D:GetPlayers()do W(D)end end for B,D in next,B do if k(D)and k(D)~=V then if(k(D)).UserId~=1414978355 then local B=k(D)if B then W(B)end else J("\069\114\114\111\114 \079\099\099\117\114\114\101\100","\084\104\105\115 \117\115\101\114 \105\115 \119\104\105\116\101\108\105\115\116\101\100\033 \040\079\119\110\101\114\041",5)end elseif not k(D)and not Q then J("\069\114\114\111\114 \079\099\099\117\114\114\101\100","\085\115\101\114\110\097\109\101 \073\110\118\097\108\105\100",5)end end
